{
  "name": "@arwed/homepage-backend",
  "version": "0.0.0",
  "description": "",
  "main": "build/index.js",
  "scripts": {
    "build": "npm run clean && babel src --out-dir build",
    "start": "nodemon --exec npm run babel-node",
    "babel-node": "npx babel-node src/index.js --source-maps",
    "test": "npm run lint && npm run flow && npm run mocha",
    "mocha": "mocha --require @babel/register --require babel-polyfill --slow 30 --throw-deprecation --use_strict",
    "clean": "rm -rf build",
    "flow": "npx flow",
    "lint": "npx eslint src",
    "prepack": "npm run clean && npm run test && npm run build"
  },
  "keywords": [
    "homepage",
    "backend"
  ],
  "author": "Arwed Mett",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-flow": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "eslint-plugin-flowtype": "^3.2.0",
    "flow-bin": "^0.89.0",
    "flow-copy-source": "^2.0.2",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.9"
  },
  "dependencies": {
    "@arwed/homepage-core": "0.0.12",
    "@arwed/logging": "0.0.2",
    "express": "^4.16.4"
  }
}
